<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.geepmd.entity.SpecialFollowUp" table="special_followup" catalog="">

        <id name="followUpId" type="java.lang.Integer">
            <column name="followUpId" />
            <generator class="identity" />
        </id>
        <property name="motherId" type="java.lang.String">
            <column name="motherId" length="50" />
        </property>
        <property name="followUpMessage" type="java.lang.String">
            <column name="followUpMessage" length="500" />
        </property>
    </class>
</hibernate-mapping>