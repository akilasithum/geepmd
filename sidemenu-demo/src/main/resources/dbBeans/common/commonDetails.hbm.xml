<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.geepmd.entity.CommonDetails" table="common_details" catalog="">
        <id name="surveyId" type="java.lang.Integer">
            <column name="surveyId" />
            <generator class="identity" />
        </id>
        <property name="motherId" type="java.lang.Integer">
            <column name="motherId"  />
        </property>
        <property name="examinorId" type="java.lang.Integer">
            <column name="examinorId"  />
        </property>
        <property name="addedDate" type="java.util.Date">
            <column name="addedDate"  />
        </property>
        <property name="surveyType" type="java.lang.String">
            <column name="surveyType" length="50"/>
        </property>
    </class>
</hibernate-mapping>